generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AnggaranTKD {
  id          Int      @id @default(autoincrement())
  jenis_tkd   String
  tahun       Int
  daerah      String // ‚Üê ini WAJIB kamu tambahkan
  semula      BigInt?
  pencadangan BigInt?
  menjadi     BigInt?
  pagu        BigInt?
  realisasi   Float?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
